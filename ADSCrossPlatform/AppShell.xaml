<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ADSCrossPlatform.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ADSCrossPlatform"
    Shell.FlyoutBehavior="Disabled"
    Title="ADSCrossPlatform">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Window_310"
        ContentTemplate="{DataTemplate local:Window_310}"
        Route="Window_310" />

    <ShellContent
    Title="Details"
    ContentTemplate="{DataTemplate local:Details}"
    Route="Details" />
</Shell>
